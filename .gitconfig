[user]
	name = Fredrik LÃ¶fgren
	email = fredrik.lofgren@weahead.se
[push]
	default = matching
[color]
	ui = auto
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	fb = checkout -b
	glog = log --graph --all --oneline --decorate
	loga = log --oneline --abbrev-commit --all
	fls = flow feature start
	flp = flow feature publish
	dsf = "!f() { [ -z \"$GIT_PREFIX\" ] || cd \"$GIT_PREFIX\" && git diff --color \"$@\" | diff-so-fancy | less --tabs=4 -RFX; }; f"
	showf = "!f() { [ -z \"$GIT_PREFIX\" ] || cd \"$GIT_PREFIX\" && git show --color \"$@\" | diff-so-fancy | less --tabs=4 -RFX; }; f"
[core]
	#pager = cat
	excludesfile = /Users/lofgrenfredrik/.gitignore_global
[rerere]
	enabled = true
[credential]
	helper = osxkeychain
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
