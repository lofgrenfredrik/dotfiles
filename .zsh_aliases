# Git
alias gclean='git branch --merged | grep -v "\*" | grep -v master | grep -v develop | xargs -n 1 git branch -d'
alias gcleanunmerged='git branch --no-merged | grep -v "\*" | grep -v master | grep -v develop | xargs -n 1 git branch -D'

